{
    "name": "datalens",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --check . && eslint .",
        "format": "prettier --write .",
        "prepare": "pnpm run smui-theme-light && pnpm run smui-theme-dark",
        "smui-theme-light": "smui-theme compile static/smui.css -i src/theme -i node_modules",
        "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark -i node_modules"
    },
    "devDependencies": {
        "@material/theme": "^14.0.0",
        "@mdi/js": "^7.4.47",
        "@smui-extra/accordion": "^7.0.0",
        "@smui/button": "^7.0.0",
        "@smui/checkbox": "^7.0.0",
        "@smui/dialog": "^7.0.0",
        "@smui/drawer": "^7.0.0",
        "@smui/fab": "^7.0.0",
        "@smui/form-field": "^7.0.0",
        "@smui/icon-button": "^7.0.0",
        "@smui/list": "^7.0.0",
        "@smui/menu": "^7.0.0",
        "@smui/radio": "^7.0.0",
        "@smui/segmented-button": "^7.0.0",
        "@smui/select": "^7.0.0",
        "@smui/switch": "^7.0.0",
        "@smui/textfield": "^7.0.0",
        "@smui/top-app-bar": "^7.0.0",
        "@sveltejs/adapter-auto": "^3.0.0",
        "@sveltejs/adapter-static": "^3.0.2",
        "@sveltejs/kit": "^2.0.0",
        "@sveltejs/vite-plugin-svelte": "^3.0.0",
        "@types/eslint": "^8.56.7",
        "autoprefixer": "^10.4.19",
        "eslint": "^9.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.36.0",
        "globals": "^15.0.0",
        "postcss": "^8.4.39",
        "prettier": "^3.1.1",
        "prettier-plugin-svelte": "^3.1.2",
        "smui-theme": "^7.0.0",
        "svelte": "^4.2.7",
        "svelte-check": "^3.6.0",
        "tailwindcss": "^3.4.6",
        "tslib": "^2.4.1",
        "typescript": "^5.0.0",
        "typescript-eslint": "^8.0.0-alpha.20",
        "vite": "^5.0.3"
    },
    "type": "module",
    "dependencies": {
        "@iconify/svelte": "^4.0.2",
        "@smui/snackbar": "^7.0.0",
        "@tauri-apps/api": "^1.6.0"
    }
}
